    "googleapis": "^148.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
